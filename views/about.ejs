                prev
              </div>
              <div
                class="swiper-button-next-nav swiper-button-next slider-custom-text-next fs-14 text-uppercase text-dark-gray fw-600 ls-1px"
              >
                next
              </div>
              <!-- end slider pagination -->
            </div>
          </div>

          <!-- Call to Action -->
          <div class="row mt-6">
            <div class="col-12 text-center">
              <span
                class="ls-2px text-uppercase text-dark-gray fw-600 lh-22 mb-10px d-block"
              >
                Need reliable welding services?
                <a
                  href="mailto:info@afzalsteelworks.com"
                  class="d-inline-block border-2 border-bottom border-color-transparent-base-color fw-700 text-dark-gray text-dark-gray-hover"
                >
                  info@afzalsteelworks.com
                </a>
              </span>
            </div>
          </div>
        </div>
      </section>
      <!-- end section -->

       <!-- start footer -->
       <%- include('partials/footer') %>
      <!-- end footer -->
    </div>
      <!-- start cookie message -->
      <%- include('partials/cookies') %>
    <!-- end cookie message -->
    <!-- start scroll progress -->
     <%- include('partials/scroll') %>
